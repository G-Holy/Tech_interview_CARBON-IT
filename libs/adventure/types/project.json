{
  "root": "libs/adventure/types",
  "sourceRoot": "libs/adventure/types/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/adventure/types/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/adventure/types"],
      "options": {
        "jestConfig": "libs/adventure/types/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
